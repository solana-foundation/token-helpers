      "type": "git",
      "url": ""
    },
    "bugs": {
      "url": ""
    },
    "homepage": "",
    "devDependencies": {
      "@types/jest": "^29.5.0",
      "@typescript-eslint/eslint-plugin": "^8.34.0",
      "@typescript-eslint/parser": "^8.34.0",
      "eslint": "^9.32.0",
      "jest": "^29.5.0",
      "ts-jest": "^29.1.0",
      "typescript": "^5.8.3"
    },
    "ava": {
      "nodeArguments": [
        "--no-warnings"
      ],
      "typescript": {
        "compile": false,
        "rewritePaths": {
          "test/": "dist/test/"
        }
      }
